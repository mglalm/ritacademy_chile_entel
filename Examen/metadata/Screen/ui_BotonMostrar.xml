<?xml version="1.0" encoding="UTF-8" ?>
<userInterface name="Screen.BotonMostrar">
  <extends>ui_com.conceptwave.system.Application</extends>
  <label>Mostrar Tabla</label>
  <metaVersion>25</metaVersion>
  <url>/BotonMostrar</url>
  <vformList>
    <vform name="Forms" type="frmui">
      <extends>ui_com.conceptwave.system.Application/frmui_Forms</extends>
      <overlayList>
        <overlay name="Menu" type="exov">
          <base>ui_com.conceptwave.system.Application/frmui_Forms/exov_Menu</base>
          <extends>ui_com.conceptwave.system.Application/frmui_Forms/exov_Menu</extends>
          <label>Menu</label>
          <overrideList>
            <override name="HorizontalLayoutForMenuItems" type="elext">
              <elementList>
                <element name="Button" type="elbtn">
                  <clickMethod type="varPath">
                    <variablePathList>
                      <variablePath>ui_Screen.BotonMostrar/usrActionMethod_buscarSQL</variablePath>
                    </variablePathList>
                  </clickMethod>
                  <label>Mostrar tabla</label>
                </element>
              </elementList>
            </override>
          </overrideList>
        </overlay>
        <overlay name="Page" type="expg">
          <base>ui_com.conceptwave.system.Application/frmui_Forms/pg_Page</base>
          <extends>ui_com.conceptwave.system.Application/frmui_Forms/pg_Page</extends>
          <label>Page</label>
          <stylesheet>/cwf/css/cwf.css</stylesheet>
          <title>Tabla de Numeros</title>
        </overlay>
      </overlayList>
    </vform>
  </vformList>
  <methodList>
    <method name="mostrarTabla" type="usrActionMethod">
      <form>ui_com.conceptwave.system.DocumentUserInterface/frmui_Forms/exov_Default</form>
      <userInterface>doc_namespaceUno.DocumentoNumeros</userInterface>
      <valueType>doc_namespaceUno.DocumentoNumeros</valueType>
    </method>
    <method name="buscarSQL" type="usrActionMethod">
      <userInterface>findSql_namespaceUno.FinderSQL</userInterface>
      <valueType>findSql_namespaceUno.FinderSQL</valueType>
    </method>
  </methodList>
</userInterface>