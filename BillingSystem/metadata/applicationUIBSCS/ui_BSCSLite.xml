      <userInterface>doc_planManagerBSCS.DOC_PLAN_BSCS</userInterface>
      <valueType>doc_planManagerBSCS.DOC_PLAN_BSCS</valueType>
    </method>
    <method name="buscarPlan" type="usrActionMethod">
      <userInterface>findDoc_planManagerBSCS.busquedaPlan</userInterface>
      <valueType>findDoc_planManagerBSCS.busquedaPlan</valueType>
    </method>
    <method name="buscarServicios" type="usrActionMethod">
      <userInterface>findDoc_provisionManagerBSCS.busquedaServicios</userInterface>
      <valueType>findDoc_provisionManagerBSCS.busquedaServicios</valueType>
    </method>
  </methodList>
</userInterface>