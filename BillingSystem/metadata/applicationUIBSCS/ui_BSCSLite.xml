    </method>
    <method name="buscarCliente" type="usrActionMethod">
      <userInterface>findDoc_customerManagerBSCS.busquedaCliente</userInterface>
      <valueType>findDoc_customerManagerBSCS.busquedaCliente</valueType>
    </method>
    <method name="selectApp" type="usrActionMethod">
      <script>UserProfile.selectApp();</script>
    </method>
    <method name="nvoMsisdn" type="usrActionMethod">
