          <extends>ui_com.conceptwave.system.Application/frmui_Forms/exov_Menu</extends>
          <label>Menu</label>
          <overrideList>
            <override name="HorizontalLayout" type="elext">
              <elementList>
                <element name="tools" type="elmnu">
                  <label>Tools</label>
                  <elementList>
                    <element name="selectApp" type="elmnu">
                      <clickMethod type="varPath">
                        <variablePathList>
                          <variablePath>ui_applicationUIBSCS.BSCSLite/usrActionMethod_selectApp</variablePath>
                        </variablePathList>
                      </clickMethod>
                      <label>Select Application</label>
                    </element>
                  </elementList>
                </element>
              </elementList>
            </override>
            <override name="HorizontalLayoutForMenuItems" type="elext">
              <elementList>
                <element name="Customers" type="elmnu">
                  <label>Clientes</label>
                  <elementList>
                    <element name="Nuevo_Cliente" type="elmnu">
                      <clickMethod type="varPath">
                        <variablePathList>
                          <variablePath>ui_applicationUIBSCS.BSCSLite/usrActionMethod_nvoCliente</variablePath>
                        </variablePathList>
                      </clickMethod>
                      <label>Crear</label>
                    </element>
                    <element name="Buscar_Cliente" type="elmnu">
                      <clickMethod type="varPath">
                        <variablePathList>
                          <variablePath>ui_applicationUIBSCS.BSCSLite/usrActionMethod_buscarCliente</variablePath>
                        </variablePathList>
                      </clickMethod>
                      <label>Buscar</label>
                    </element>
                  </elementList>
                </element>
                <element name="Msisdn" type="elmnu">
                  <label>MSISDN</label>
                  <elementList>
                    <element name="Nuevo_Msisdn" type="elmnu">
                      <clickMethod type="varPath">
                        <variablePathList>
                          <variablePath>ui_applicationUIBSCS.BSCSLite/usrActionMethod_nvoMsisdn</variablePath>
                        </variablePathList>
                      </clickMethod>
                      <label>Crear</label>
                    </element>
                    <element name="Buscar_Msisdn" type="elmnu">
                      <clickMethod type="varPath">
                        <variablePathList>
                          <variablePath>ui_applicationUIBSCS.BSCSLite/usrActionMethod_buscarMsisdn</variablePath>
                        </variablePathList>
                      </clickMethod>
                      <label>Buscar</label>
                    </element>
                  </elementList>
                </element>
                <element name="Planes" type="elmnu">
                  <label>Planes</label>
                  <elementList>
                    <element name="Nuevo_Plan" type="elmnu">
                      <clickMethod type="varPath">
                        <variablePathList>
                          <variablePath>ui_applicationUIBSCS.BSCSLite/usrActionMethod_nvoPlan</variablePath>
                        </variablePathList>
                      </clickMethod>
                      <label>Crear</label>
                    </element>
                    <element name="Buscar_Plan" type="elmnu">
                      <clickMethod type="varPath">
                        <variablePathList>
                          <variablePath>ui_applicationUIBSCS.BSCSLite/usrActionMethod_buscarPlan</variablePath>
                        </variablePathList>
                      </clickMethod>
                      <label>Buscar</label>
                    </element>
                  </elementList>
                </element>
                <element name="Servicios" type="elmnu">
                  <label>Servicios Por Clientes</label>
                  <elementList>
                    <element name="Servicios_Clientes" type="elmnu">
                      <clickMethod type="varPath">
                        <variablePathList>
                          <variablePath>ui_applicationUIBSCS.BSCSLite/usrActionMethod_buscarServicios</variablePath>
                        </variablePathList>
                      </clickMethod>
                      <label>Servicios Por Cliente</label>
                    </element>
                  </elementList>
                </element>
              </elementList>
            </override>
          </overrideList>
        </overlay>
        <overlay name="Page" type="expg">
          <base>ui_com.conceptwave.system.Application/frmui_Forms/pg_Page</base>
