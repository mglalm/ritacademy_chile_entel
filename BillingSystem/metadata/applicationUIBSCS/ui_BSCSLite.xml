    <method name="buscarMsisdn" type="usrActionMethod">
      <userInterface>findDoc_msisdnManagerBSCS.busquedaMsisdn</userInterface>
      <valueType>findDoc_msisdnManagerBSCS.busquedaMsisdn</valueType>
