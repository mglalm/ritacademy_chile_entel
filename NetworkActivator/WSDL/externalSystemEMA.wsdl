<?xml version="1.0" encoding="UTF-8" ?>
<definitions name="externalSystemEMA" targetNamespace="externalSystemEMA"
	xmlns:tns="externalSystemEMA"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
	xmlns="http://schemas.xmlsoap.org/wsdl/"
	xmlns:externalSystemEMA="externalSystemEMA">

	<types>
		<schema targetNamespace="externalSystemEMA"			
	xmlns="http://www.w3.org/2001/XMLSchema"			
	xmlns:externalSystemEMA="externalSystemEMA"			
	xmlns:xsd="http://www.w3.org/2001/XMLSchema">



			<annotation>
				<documentation>External System EMA</documentation>
				<appinfo>
					<metadata>AVM Metadata CS</metadata>
					<created>Wed Feb 22 13:53:02 CLT 2017</created>
				</appinfo>
			</annotation>

			<!-- Data types and elements -->

			<simpleType name="ID_SERVICIO_SERVICO_CS">
				<annotation>
					<documentation>ID_SERVICIO_SERVICO_CS</documentation>
				</annotation>
				<restriction base="xsd:decimal">
					<totalDigits value="5"/>
				</restriction>
			</simpleType>

			<simpleType name="ORDER_ID_SERVICIO_CS">
				<annotation>
					<documentation>ORDER_ID_SERVICIO_CS</documentation>
				</annotation>
				<restriction base="xsd:string">
					<maxLength value="30"/>
				</restriction>
			</simpleType>

			<simpleType name="ID_PLAN_SERVICIO_CS">
				<annotation>
					<documentation>ID_PLAN_SERVICIO_CS</documentation>
				</annotation>
				<restriction base="xsd:decimal">
					<totalDigits value="5"/>
				</restriction>
			</simpleType>

			<simpleType name="MSISDN_SERVICIO_CS">
				<annotation>
					<documentation>MSISDN_SERVICIO_CS</documentation>
				</annotation>
				<restriction base="xsd:string">
					<maxLength value="15"/>
