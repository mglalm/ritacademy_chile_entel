			</simpleType>

			<simpleType name="ID_SERVICIO_ESTADO_SERVICO_CS">
				<annotation>
					<documentation>ID_SERVICIO_ESTADO_SERVICO_CS</documentation>
				</annotation>
				<restriction base="xsd:string">
					<maxLength value="1"/>
