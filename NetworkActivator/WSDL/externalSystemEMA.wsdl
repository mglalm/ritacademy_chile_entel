			</simpleType>

			<simpleType name="FECHA_ACTIVACION_SERVICIO_CS">
				<annotation>
					<documentation>FECHA_ACTIVACION_SERVICIO_CS</documentation>
				</annotation>
				<restriction base="xsd:dateTime">
