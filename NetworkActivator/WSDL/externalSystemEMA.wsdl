				</complexType>

			<complexType name="output_provServicioEMA">
				<annotation>
					<documentation>output_provServicioEMA</documentation>
				</annotation>
					<sequence>
						<element name="estado" minOccurs="0" maxOccurs="1" type="xsd:string"/>
						<element name="mensaje" minOccurs="0" maxOccurs="1" type="xsd:string"/>
					</sequence>
				</complexType>

			<!-- Documents -->

			<complexType name="DOC_SERVICIO_GET2">
				<annotation>
					<documentation>DOC_SERVICIO_GET2</documentation>
				</annotation>
					<sequence>
						<element name="ID_SERVICIO_SERVICO_CS" type="externalSystemEMA:ID_SERVICIO_SERVICO_CS"/>
						<element name="ID_PLAN_SERVICIO_CS" type="externalSystemEMA:ID_PLAN_SERVICIO_CS"/>
						<element name="ID_SERVICIO_ESTADO_SERVICO_CS" type="externalSystemEMA:ID_SERVICIO_ESTADO_SERVICO_CS"/>
						<element name="MSISDN_SERVICIO_CS" type="externalSystemEMA:MSISDN_SERVICIO_CS"/>
						<element name="FECHA_ACTIVACION_SERVICIO_CS" type="externalSystemEMA:FECHA_ACTIVACION_SERVICIO_CS"/>
