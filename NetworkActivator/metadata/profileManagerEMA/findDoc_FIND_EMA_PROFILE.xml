    <methodList>
      <method name="btn_nodosactivos" type="usrActionMethod">
        <dialogHeight>400</dialogHeight>
        <dialogWidth>2700</dialogWidth>
        <showInPopup>true</showInPopup>
        <script><![CDATA[
          externalSystemEMA.quiebra_ejecucion();
          if (this.result.selected!=null && this.result.selected.length == 1)
          {
              var var_valor = this.result.selected[0].ID_PROFILE_N;
              var finder = new Finder("profileManagerEMA.FIND_EMA_NODO_PROFILE_POPUP");
              finder.searchDocument.ID_PROFILE_N = var_valor;

              var res = finder.search();

              var finderUI = new profileManagerEMA.FIND_EMA_NODO_PROFILE_POPUP.UserInterface(null, this);
              finderUI.result = res;

              return finderUI;
          }
          else
              Global.showUserMessage("Debe seleccionar un registro para visualizar");
        ]]></script>
      </method>
      <method name="btn_editar" type="usrActionMethod">
        <script><![CDATA[
          if (this.result.selected!=null && this.result.selected.length == 1)
          {

              if (this.result.selected!=null && this.result.selected.length == 1)
              {
                this.detail = new this.result.selected[0].UserInterface(this.result.selected[0], this);
                this.isSearchButtonVisible = false;
              }
          }
          else
              Global.showUserMessage("Debe seleccionar un registro para editar");
        ]]></script>
      </method>
