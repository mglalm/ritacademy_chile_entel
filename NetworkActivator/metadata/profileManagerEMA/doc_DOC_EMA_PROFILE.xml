    <methodList>
      <method name="btn_guardar" type="usrActionMethod">
        <script><![CDATA[
          if ( this.model.ID_PROFILE_C != null && this.model.ID_PROFILE_C != "" )
          {
              if ( this.model.ID_PROFILE_N == null )
              {
                  //-----OBTIENE NUEVA SECUENCIA PARA EL NUEVO PLAN
                  var doc_secuencia = new Finder("profileManagerEMA.SEQ_ID_PERFIL_EMA");
                  var nueva_secuencia = doc_secuencia.search();
                  var var_id_valor=nueva_secuencia[0].type;
                  this.model.ID_PROFILE_N=var_id_valor;
              }

              this.save();
              Global.showUserMessage("Perfil Almacenado");

              var finderUI = new profileManagerEMA.FIND_EMA_PROFILE.UserInterface(null, this);
              return finderUI;

          }
          else
              Global.showUserMessage("Debe ingresar un ID");
        ]]></script>
      </method>
