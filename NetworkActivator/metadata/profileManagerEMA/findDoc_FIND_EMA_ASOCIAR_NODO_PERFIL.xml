    <methodList>
      <method name="btn_guardar" type="usrActionMethod">
        <script><![CDATA[
          externalSystemEMA.quiebra_ejecucion();

          var DT_EMA_ID_NODO_N;
          var DT_EMA_ID_NODO_C;
          var DT_EMA_ID_PROFILE_N;
          var DT_EMA_ID_PROFILE_C;
          var DT_NODO_PERFIL_CONFIGURADO;
          var DT_EMA_DESC_PROFILE_DETALLE;
          var DT_EMA_PLATAFORMA;

          if ( this.result.length > 0 )
          {
                //-----PARTE DEL BORRADO
                DT_EMA_ID_NODO_N=this.result[0].DT_EMA_ID_NODO_N;
                DT_EMA_ID_PROFILE_N=this.result[0].DT_EMA_ID_PROFILE_N;

                //----BORRA TODO PARA EL PERFIL
                var listado_pares = new Finder("profileManagerEMA.FIND_EMA_PROFILE_DETALLE");
                var resultado = listado_pares.search();
                if ( resultado.length > 0)
                {
                      for ( var j = 0 ; j < resultado.length ; j++ )
                      {
                            if ( resultado[j].DT_EMA_ID_PROFILE_N == DT_EMA_ID_PROFILE_N )
                            {
                                  //-----SI LO ENCUENTRA ELIMINA EL REGISTRO
                                  resultado[j].deleteItem();
                                  //resultado[j].save();
                            }
                      }
                }

                //-----AHORA HAY QUE CREAR LOS REGISTROS PARA LOS QUE SE INDICAN COMO SI
              for (var i=0 ;    i < this.result.length ; i++ )
              {
                      DT_EMA_ID_NODO_N=this.result[i].DT_EMA_ID_NODO_N;
                      DT_EMA_ID_NODO_C=this.result[i].DT_EMA_ID_NODO_C;
                      DT_EMA_ID_PROFILE_N=this.result[i].DT_EMA_ID_PROFILE_N;
                      DT_EMA_ID_PROFILE_C=this.result[i].DT_EMA_ID_PROFILE_C;
                      DT_NODO_PERFIL_CONFIGURADO=this.result[i].DT_NODO_PERFIL_CONFIGURADO;
                      DT_EMA_DESC_PROFILE_DETALLE=this.result[i].DT_EMA_DESC_PROFILE_DETALLE;

                      if ( DT_NODO_PERFIL_CONFIGURADO == 1 )
                      {
                            var listado_nodos = new Finder("profileManagerEMA.FIND_EMA_LISTADO_NODOS");
                            var resultado_nodos = listado_nodos.search();
                            if ( resultado_nodos.length > 0)
                            {
                                  for ( var k = 0 ; k < resultado_nodos.length ; k++ )
                                  {
                                        if ( resultado_nodos[k].DT_EMA_ID_NODO_N == DT_EMA_ID_NODO_N )
                                        {
                                              DT_EMA_PLATAFORMA= resultado_nodos[k].DT_EMA_PLATAFORMA;   // AQUI DEBIESE BUSCAR PLATAFORMAS EN NODOS
                                        }
                                  }
                            }

                            var documento = new Document("profileManagerEMA.DOC_EMA_PROFILE_DETALLE");
                            documento.DT_EMA_ID_PROFILE_N=DT_EMA_ID_PROFILE_N;
                            documento.DT_EMA_ID_PROFILE_C=DT_EMA_ID_PROFILE_C;
                            documento.DT_EMA_ID_NODO_N=DT_EMA_ID_NODO_N;
                            documento.DT_EMA_ID_NODO_C=DT_EMA_ID_NODO_C;
                            documento.DT_EMA_DESC_PROFILE_DETALLE=DT_EMA_PLATAFORMA;
                            documento.save();
                      }
                }
                Global.showUserMessage("ConfiguraciÃ³n realizada");
          }
        ]]></script>
