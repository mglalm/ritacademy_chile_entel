<?xml version="1.0" encoding="UTF-8" ?>
<userInterface name="applicationUIEMA.EMALite">
  <extends>ui_com.conceptwave.system.Application</extends>
  <label>EMA</label>
  <metaVersion>25</metaVersion>
  <url>/EMA</url>
  <vformList>
    <vform name="Forms" type="frmui">
      <extends>ui_com.conceptwave.system.Application/frmui_Forms</extends>
      <overlayList>
        <overlay name="Menu" type="exov">
          <base>ui_com.conceptwave.system.Application/frmui_Forms/exov_Menu</base>
          <extends>ui_com.conceptwave.system.Application/frmui_Forms/exov_Menu</extends>
          <label>Menu</label>
          <overrideList>
            <override name="HorizontalLayout" type="elext">
              <elementList>
                <element name="tools" type="elmnu">
                  <label>Aplicaciones</label>
                  <elementList>
                    <element name="selectApp" type="elmnu">
                      <clickMethod type="varPath">
                        <variablePathList>
                          <variablePath>ui_applicationUIEMA.EMALite/usrActionMethod_selectApp</variablePath>
                        </variablePathList>
                      </clickMethod>
                      <label>Seleccionar Aplicación</label>
                    </element>
                  </elementList>
                </element>
              </elementList>
            </override>
            <override name="HorizontalLayoutForMenuItems" type="elext">
              <elementList>
                <element name="Profile" type="elmnu">
                  <label>Perfiles</label>
                  <elementList>
                    <element name="Nuevo_Profile" type="elmnu">
                      <clickMethod type="varPath">
                        <variablePathList>
                          <variablePath>ui_applicationUIEMA.EMALite/usrActionMethod_nvoProfile</variablePath>
                        </variablePathList>
                      </clickMethod>
                      <label>Crear</label>
                    </element>
                    <element name="Buscar_Profile" type="elmnu">
                      <clickMethod type="varPath">
                        <variablePathList>
                          <variablePath>ui_applicationUIEMA.EMALite/usrActionMethod_buscarProfile</variablePath>
                        </variablePathList>
                      </clickMethod>
                      <label>Buscar</label>
                    </element>
                  </elementList>
                </element>
                <element name="Nodos" type="elmnu">
                  <label>Nodos</label>
                  <elementList>
                    <element name="Crear" type="elmnu">
                      <clickMethod type="varPath">
                        <variablePathList>
                          <variablePath>ui_applicationUIEMA.EMALite/usrActionMethod_nvoNodo</variablePath>
                        </variablePathList>
                      </clickMethod>
                      <label>Crear</label>
                    </element>
                    <element name="Buscar" type="elmnu">
                      <clickMethod type="varPath">
                        <variablePathList>
                          <variablePath>ui_applicationUIEMA.EMALite/usrActionMethod_buscarNodo</variablePath>
                        </variablePathList>
                      </clickMethod>
                      <label>Buscar</label>
                    </element>
                  </elementList>
                </element>
                <element name="MenuNodosPerfiles" type="elmnu">
                  <label>Nodos/Perfil</label>
                  <visible type="varPath">
                    <variablePathList>
                      <variablePath>dtype_com.conceptwave.system.Boolean/leaf_FALSE</variablePath>
                    </variablePathList>
                  </visible>
                  <elementList>
                    <element name="Asociar" type="elmnu">
                      <label>Asociar Nodo/Perfil</label>
                    </element>
                  </elementList>
                </element>
                <element name="Servicios" type="elmnu">
                  <label>Servicios Por Móvil</label>
                  <elementList>
                    <element name="Servicios_Clientes" type="elmnu">
                      <clickMethod type="varPath">
                        <variablePathList>
                          <variablePath>ui_applicationUIEMA.EMALite/usrActionMethod_buscarServicios</variablePath>
                        </variablePathList>
                      </clickMethod>
                      <label>Servicios Por Móvil</label>
                    </element>
                  </elementList>
                </element>
              </elementList>
            </override>
          </overrideList>
        </overlay>
        <overlay name="Page" type="expg">
          <base>ui_com.conceptwave.system.Application/frmui_Forms/pg_Page</base>
          <extends>ui_com.conceptwave.system.Application/frmui_Forms/pg_Page</extends>
          <label>Page</label>
          <stylesheet>/cwf/css/cwf.css</stylesheet>
          <title>EMA</title>
        </overlay>
      </overlayList>
    </vform>
  </vformList>
  <methodList>
    <method name="nvoProfile" type="usrActionMethod">
      <form>doc_profileManagerEMA.DOC_EMA_PROFILE/ui_UserInterface/frmui_Forms/vform_Formulario_Creacion</form>
      <userInterface>doc_profileManagerEMA.DOC_EMA_PROFILE</userInterface>
      <valueType>doc_profileManagerEMA.DOC_EMA_PROFILE</valueType>
    </method>
    <method name="buscarProfile" type="usrActionMethod">
      <userInterface>findDoc_profileManagerEMA.FIND_EMA_PROFILE</userInterface>
      <valueType>findDoc_profileManagerEMA.FIND_EMA_PROFILE</valueType>
    </method>
    <method name="nvoNodo" type="usrActionMethod">
      <form>doc_profileManagerEMA.DOC_EMA_NODO/ui_UserInterface/frmui_Forms/vform_Formulario_Creacion</form>
      <userInterface>doc_profileManagerEMA.DOC_EMA_NODO</userInterface>
      <valueType>doc_profileManagerEMA.DOC_EMA_NODO</valueType>
    </method>
    <method name="buscarNodo" type="usrActionMethod">
      <userInterface>findDoc_profileManagerEMA.FIND_EMA_NODO</userInterface>
      <valueType>findDoc_profileManagerEMA.FIND_EMA_NODO</valueType>
    </method>
    <method name="buscarServicios" type="usrActionMethod">
      <userInterface>findDoc_provisionManagerEMA.FIND_EMA_SERVICIOS_VIGENTES</userInterface>
      <valueType>findDoc_provisionManagerEMA.FIND_EMA_SERVICIOS_VIGENTES</valueType>
    </method>
    <method name="selectApp" type="usrActionMethod">
      <script>UserProfile.selectApp();</script>
    </method>
  </methodList>
</userInterface>