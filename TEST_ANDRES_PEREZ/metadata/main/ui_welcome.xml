<?xml version="1.0" encoding="UTF-8" ?>
<userInterface name="main.welcome">
  <extends>ui_com.conceptwave.system.Application</extends>
  <label>welcome</label>
  <metaVersion>25</metaVersion>
  <url>/welcome</url>
  <vformList>
    <vform name="Forms" type="frmui">
      <extends>ui_com.conceptwave.system.Application/frmui_Forms</extends>
      <overlayList>
        <overlay name="Menu" type="exov">
          <base>ui_com.conceptwave.system.Application/frmui_Forms/exov_Menu</base>
          <extends>ui_com.conceptwave.system.Application/frmui_Forms/exov_Menu</extends>
          <label>Menu</label>
          <overrideList>
            <override name="HorizontalLayoutForMenuItems" type="elext">
              <elementList>
                <element name="MenuItem" type="elmnu">
                  <label>Items Tabla</label>
                  <elementList>
                    <element name="MenuItem2" type="elmnu">
                      <clickMethod type="varPath">
                        <variablePathList>
                          <variablePath>ui_main.welcome/usrActionMethod_mostrarItem</variablePath>
                        </variablePathList>
                      </clickMethod>
                      <label>Mostrar Items</label>
                    </element>
                    <element name="MenuItem1" type="elmnu">
                      <clickMethod type="varPath">
                        <variablePathList>
                          <variablePath>ui_main.welcome/usrActionMethod_agregarItem</variablePath>
                        </variablePathList>
                      </clickMethod>
                      <label>Agregar Item</label>
                    </element>
                    <element name="MenuItem3" type="elmnu">
                      <clickMethod type="varPath">
                        <variablePathList>
                          <variablePath>ui_main.welcome/usrActionMethod_iniciaProceso</variablePath>
                        </variablePathList>
                      </clickMethod>
                      <label>Inicia Proceso Manual</label>
                    </element>
                    <element name="MenuItem4" type="elmnu">
                      <clickMethod type="varPath">
                        <variablePathList>
                          <variablePath>ui_main.welcome/usrActionMethod_invoca</variablePath>
                        </variablePathList>
                      </clickMethod>
                      <label>Invoca Proyecto 2</label>
                    </element>
                  </elementList>
                </element>
              </elementList>
            </override>
            <override name="WrkList" type="elext"/>
          </overrideList>
        </overlay>
        <overlay name="Page" type="expg">
          <base>ui_com.conceptwave.system.Application/frmui_Forms/pg_Page</base>
          <extends>ui_com.conceptwave.system.Application/frmui_Forms/pg_Page</extends>
          <label>Page</label>
          <stylesheet>/cwf/css/cwf.css</stylesheet>
          <title>PRUEBA</title>
        </overlay>
      </overlayList>
    </vform>
  </vformList>
  <methodList>
    <method name="agregarItem" type="usrActionMethod">
      <script>main.generador();</script>
    </method>
    <method name="mostrarItem" type="usrActionMethod">
      <userInterface>findDoc_main.tablaFinder</userInterface>
      <valueType>findDoc_main.tablaFinder</valueType>
    </method>
    <method name="iniciaProceso" type="usrActionMethod">
      <script><![CDATA[
        var proceso = Process.startProcess("main.procesoLimpiador",null,null,null);
        Global.showUserMessage("proceso iniciado");
      ]]></script>
    </method>
    <method name="invoca" type="usrActionMethod">
      <script><![CDATA[
        main.dummy();
        var dt = new DataStructure('consumer.request');
        dt.pregunta = "hola mundo";

        var arr = new Array();
        arr[0] = dt;
        arr[1] = null;

        var resp = Global.invokeInterface('consumer.interfaz','operacion',arr);


        Global.showUserMessage("respuesta obtenida desde el WS: "+resp[0].respuesta);
      ]]></script>
    </method>
  </methodList>
</userInterface>