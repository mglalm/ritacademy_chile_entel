<?xml version="1.0" encoding="UTF-8" ?>
<userInterface name="UIBSCS.UIBSCS">
  <extends>ui_com.conceptwave.system.Application</extends>
  <label>UI BSCS</label>
  <metaVersion>25</metaVersion>
  <url>/BSCS</url>
  <vformList>
    <vform name="Forms" type="frmui">
      <extends>ui_com.conceptwave.system.Application/frmui_Forms</extends>
      <overlayList>
        <overlay name="Menu" type="exov">
          <base>ui_com.conceptwave.system.Application/frmui_Forms/exov_Menu</base>
          <extends>ui_com.conceptwave.system.Application/frmui_Forms/exov_Menu</extends>
          <label>Menu</label>
          <overrideList>
            <override name="HorizontalLayoutForMenuItems" type="elext">
              <elementList>
                <element name="estadoDeRecursos" type="elmnu">
                  <label>Estado de Recursos</label>
                  <elementList>
                    <element name="estadoMSISDN" type="elmnu">
                      <clickMethod type="varPath">
                        <variablePathList>
                          <variablePath>ui_UIBSCS.UIBSCS/usrActionMethod_estadoDeMSISDN</variablePath>
                        </variablePathList>
                      </clickMethod>
                      <label>Estado MSISDN</label>
                    </element>
                    <element name="estadoIMSI" type="elmnu">
                      <clickMethod type="varPath">
                        <variablePathList>
                          <variablePath>ui_UIBSCS.UIBSCS/usrActionMethod_estadoDeIMSI</variablePath>
                        </variablePathList>
                      </clickMethod>
                      <label>Estado IMSI</label>
                    </element>
                  </elementList>
                </element>
                <element name="busquedaCliente" type="elmnu">
                  <label>Busqueda de Cliente</label>
                  <elementList>
                    <element name="buscarCliente" type="elmnu">
                      <clickMethod type="varPath">
                        <variablePathList>
                          <variablePath>ui_UIBSCS.UIBSCS/usrActionMethod_busquedaCliente</variablePath>
                        </variablePathList>
                      </clickMethod>
                      <label>Buscar Cliente</label>
                    </element>
                    <element name="verTrafico" type="elmnu">
                      <clickMethod type="varPath">
                        <variablePathList>
                          <variablePath>ui_UIBSCS.UIBSCS/usrActionMethod_detalleTrafico</variablePath>
                        </variablePathList>
                      </clickMethod>
                      <label>Ver tr√°fico</label>
                    </element>
                  </elementList>
                </element>
              </elementList>
            </override>
          </overrideList>
        </overlay>
        <overlay name="Page" type="expg">
          <base>ui_com.conceptwave.system.Application/frmui_Forms/pg_Page</base>
          <extends>ui_com.conceptwave.system.Application/frmui_Forms/pg_Page</extends>
          <label>Page</label>
          <skin>Cupertino</skin>
          <stylesheet>/cwf/css/cwf.css</stylesheet>
          <title>BSCS</title>
        </overlay>
      </overlayList>
    </vform>
  </vformList>
  <methodList>
    <method name="estadoDeMSISDN" type="usrActionMethod">
      <form>findDoc_bscsProy.fndBusquedaMSISDN/ui_UserInterface/frmui_Forms/exov_Default</form>
      <userInterface>findDoc_bscsProy.fndBusquedaMSISDN</userInterface>
      <valueType>findDoc_bscsProy.fndBusquedaMSISDN</valueType>
    </method>
    <method name="estadoDeIMSI" type="usrActionMethod">
      <form>findDoc_bscsProy.findBusquedaIMSI/ui_UserInterface/frmui_Forms/exov_Default</form>
      <userInterface>findDoc_bscsProy.findBusquedaIMSI</userInterface>
      <valueType>findDoc_bscsProy.findBusquedaIMSI</valueType>
    </method>
    <method name="busquedaCliente" type="usrActionMethod">
      <form>findDoc_bscsProy.busquedaCliente/ui_UserInterface/frmui_Forms/exov_Default</form>
      <userInterface>findDoc_bscsProy.busquedaCliente</userInterface>
      <valueType>findDoc_bscsProy.busquedaCliente</valueType>
    </method>
    <method name="detalleTrafico" type="usrActionMethod">
      <form>ui_com.conceptwave.system.FinderUserInterface/frmui_Forms/vform_ReferenceForm</form>
      <showInPopup>true</showInPopup>
      <userInterface>findDoc_bscsProy.fndDetalleTrafico</userInterface>
      <valueType>findDoc_bscsProy.fndDetalleTrafico</valueType>
    </method>
  </methodList>
</userInterface>