                          </variablePathList>
                        </variable>
                      </element>
                      <element name="customerAddress" type="elfld">
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findDoc_customerManagement.customerFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_customerManagement.customerProfile/leaf_customerAddress</variablePath>
                          </variablePathList>
                        </variable>
                      </element>
                      <element name="customerEmail" type="elfld">
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findDoc_customerManagement.customerFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_customerManagement.customerProfile/leaf_customerEmail</variablePath>
                          </variablePathList>
                        </variable>
                      </element>
                      <element name="customerMsisdn" type="elfld">
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findDoc_customerManagement.customerFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_customerManagement.customerProfile/leaf_customerMsisdn</variablePath>
                          </variablePathList>
                        </variable>
                      </element>
                      <element name="cwCreatedBy" type="elfld">
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findDoc_customerManagement.customerFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_customerManagement.customerProfile/leaf_cwCreatedBy</variablePath>
                          </variablePathList>
                        </variable>
                      </element>
                      <element name="cwOrderCreationDate" type="eldate">
                        <variable type="varPath">
                          <variablePathList>
                            <variablePath>findDoc_customerManagement.customerFinder/ui_UserInterface/uivar_result</variablePath>
                            <variablePath>doc_customerManagement.customerProfile/leaf_cwOrderCreationDate</variablePath>
                          </variablePathList>
                        </variable>
                        <visible type="varPath">
                          <variablePathList>
                            <variablePath>dtype_com.conceptwave.system.Boolean/leaf_FALSE</variablePath>
                          </variablePathList>
                        </visible>
                      </element>
                    </elementList>
                  </element>
                </elementList>
              </override>
            </overrideList>
          </overlay>
        </overlayList>
      </vform>
    </vformList>
  </UserInterface>
  <viewList>
    <view name="customerFinderView" type="findVView">
      <inputOverlay>doc_customerManagement.customerSearch/ui_UserInterface/frmui_Forms/vform_generatedForm</inputOverlay>
      <outputOverlay>findDoc_customerManagement.customerFinder/ui_UserInterface/frmui_Forms/exov_Result</outputOverlay>
      <searchImmediately>true</searchImmediately>
      <showSearchForm>true</showSearchForm>
    </view>
  </viewList>
</findDoc>